<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.sdr.bbs.model.Bbs" />
	<import class="com.sdr.bbs.model.BbsComment" />
	<import class="com.sdr.bbs.model.BbsPost" />
	<class name="com.sdr.bbs.model.impl.BbsImpl" table="sdr_bbs_Bbs">
		<cache usage="read-write" />
		<id name="bbsId" type="long">
			<generator class="assigned" />
		</id>
		<property name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" column="uuid_" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="title" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="displayType" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
	</class>
	<class name="com.sdr.bbs.model.impl.BbsCommentImpl" table="sdr_bbs_BbsComment">
		<cache usage="read-write" />
		<id name="commentId" type="long">
			<generator class="assigned" />
		</id>
		<property name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" column="uuid_" />
		<property name="postId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="writer" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="email" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="password" type="com.liferay.portal.dao.orm.hibernate.StringType" column="password_" />
		<property name="content" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="secret" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
	</class>
	<class name="com.sdr.bbs.model.impl.BbsPostImpl" table="sdr_bbs_BbsPost">
		<cache usage="read-write" />
		<id name="postId" type="long">
			<generator class="assigned" />
		</id>
		<property name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" column="uuid_" />
		<property name="bbsId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="writer" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="email" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="password" type="com.liferay.portal.dao.orm.hibernate.StringType" column="password_" />
		<property name="title" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="content" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="secret" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="commentEmailed" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="viewCount" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
	</class>
</hibernate-mapping>